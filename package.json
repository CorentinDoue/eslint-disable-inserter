{
  "name": "eslint-disable-inserter",
  "version": "0.3.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/CorentinDoue/eslint-disable-inserter",
  "author": "Steven Petryk and Corentin Dou√©",
  "scripts": {
    "clean-dist": "rm -rf dist && mkdir dist && cp src/eslint.schema.json dist",
    "start": "tsc --watch",
    "build": "yarn run clean-dist && tsc",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js --runInBand",
    "prettier": "prettier . --write",
    "test:type": "tsc --noEmit"
  },
  "bin": "dist/cli.js",
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier"
    }
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "fs-extra": "^9.0.1",
    "get-stdin": "^8.0.0",
    "jsonschema": "^1.2.6",
    "meow": "^7.1.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/get-stdin": "^7.0.0",
    "@types/jest": "^26.0.14",
    "@types/jsonschema": "^1.1.1",
    "@types/meow": "^5.0.0",
    "@types/node": "^14.11.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "semantic-release": "^17.3.3",
    "ts-jest": "^26.3.0"
  },
  "packageManager": "yarn@4.0.2"
}
